  $(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\coredll.lib \
  $(VIVANTE_SDK_LIB)\libEGL.lib \
  $(VIVANTE_SDK_LIB)\libOpenVG.lib \
  $(VIVANTE_SDK_LIB)\libVDK.lib \

